<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/verde_spinner" />
            <corners android:radius="8dp" />
            <stroke android:width="1dp" android:color="@color/verde_spinner_borda" />


        </shape>
    </item>
    <item android:right="16dp" android:gravity="end|center_vertical">
        <bitmap
            android:color="@color/verde_button"
            android:src="@android:drawable/arrow_down_float"
            android:gravity="center"/></item>
</layer-list>

